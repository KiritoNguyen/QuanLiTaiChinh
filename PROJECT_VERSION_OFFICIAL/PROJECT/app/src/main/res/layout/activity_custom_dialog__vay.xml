<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asus.project.CustomDialog_vay">

    <TextView
        android:id="@+id/tvcustom_vay"
        android:layout_width="95dp"
        android:layout_height="34dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        android:text="Giải  trí"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Spinner
        android:id="@+id/spinnercustom_vay"
        android:layout_width="25dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toEndOf="@+id/tvcustom_vay"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/edtmoneycustom_vay"
        android:layout_width="129dp"
        android:layout_height="41dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="148dp"
        android:hint="SỐ TIỀN"
        app:layout_constraintEnd_toStartOf="@+id/edtlaysuatcustom_vay"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edtlaysuatcustom_vay"
        android:layout_width="98dp"
        android:layout_height="45dp"
        android:layout_marginEnd="76dp"
        android:layout_marginTop="148dp"
        android:hint="LÃI SUẤT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edt_ghichuvaycustom"
        android:layout_width="147dp"
        android:layout_height="39dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="88dp"
        android:hint="NOTE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtcalendarcustom_vay"
        android:layout_width="133dp"
        android:layout_height="46dp"
        android:layout_alignBottom="@+id/btncalendarcustom_vay"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="220dp"
        android:layout_toLeftOf="@+id/btncalendarcustom_vay"
        android:layout_toStartOf="@+id/btncalendarcustom_vay"
        android:text="  "
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/btncalendarcustom_vay"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtcalendarcustomngtra_vay"
        android:layout_width="132dp"
        android:layout_height="46dp"
        android:layout_alignBottom="@+id/btncalendarcustom_vay"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_toLeftOf="@+id/btncalendarcustom_vay"
        android:layout_toStartOf="@+id/btncalendarcustom_vay"
        android:text="  "
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/btncalendarcustomngtra_vay"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtcalendarcustom_vay" />

    <Button
        android:id="@+id/btncalendarcustom_vay"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btncancelcustom_vay"
        android:layout_marginStart="208dp"
        android:layout_marginTop="220dp"
        android:layout_toEndOf="@+id/edtmoneycustom_vay"
        android:layout_toRightOf="@+id/edtmoneycustom_vay"
        android:text="Ngày Vay"
        android:drawableLeft="@drawable/date"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btncalendarcustomngtra_vay"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btncancelcustom_vay"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/edtmoneycustom_vay"
        android:layout_toRightOf="@+id/edtmoneycustom_vay"
        android:drawableLeft="@drawable/date"
        android:text="Ngày Trả"
        app:layout_constraintBottom_toTopOf="@+id/btncancelcustom_vay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.035"
        app:layout_constraintLeft_toRightOf="@+id/txtcalendarcustom_vay"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btncancelcustom_vay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btndeletecustom_vay"
        android:layout_alignBottom="@+id/btndeletecustom_vay"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="360dp"
        android:layout_toEndOf="@+id/btndeletecustom_vay"
        android:layout_toRightOf="@+id/btndeletecustom_vay"
        android:text="Cancel"
        app:layout_constraintEnd_toStartOf="@+id/btnupdatecustom_vay"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btndeletecustom_vay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnupdatecustom_vay"
        android:layout_alignBottom="@+id/btnupdatecustom_vay"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="360dp"
        android:layout_toEndOf="@+id/btnupdatecustom_vay"
        android:layout_toRightOf="@+id/btnupdatecustom_vay"
        android:text="Delete"
        app:layout_constraintEnd_toEndOf="@+id/txtcalendarcustom_vay"
        app:layout_constraintHorizontal_bias="0.329"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnupdatecustom_vay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tvcustom_vay"
        android:layout_alignRight="@+id/tvcustom_vay"
        android:layout_below="@+id/edtmoneycustom_vay"
        android:layout_marginEnd="44dp"
        android:layout_marginTop="360dp"
        android:text="Update"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
